<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/dev_options"
        android:key="pref_dev">
        <CheckBoxPreference
            android:key="dev_manifest_url_override"
            android:title="@string/pref_manifest_override"
            android:defaultValue="false"/>
        <EditTextPreference
            android:key="dev_manifest_url_v2"
            android:title="@string/pref_manifest_url"
            android:dependency="dev_manifest_url_override"/>
        <EditTextPreference
            android:key="dev_device_name"
            android:title="@string/pref_device_name"
            android:summary="@string/dangerous"/>
    </PreferenceCategory>
</PreferenceScreen>
